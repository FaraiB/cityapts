<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/home.css" />
    <script type="text/javascript">
      document.getElementById("button").onclick = function () {
        location.href = "/property.ejs";
      };
    </script>
    <title>Home Page</title>
  </head>
  <body>
    <nav class="logo">
      <a href="/home.ejs">
        <img src="/logo/logotipo.svg" />
      </a>
    </nav>
    <div class="banner"></div>
    <div class="container">
      <p class="text">Selecione o imóvel desejado</p>
      <div class="search">
        <div>
          <form name="properties" action="/property.ejs">
            <select name="properties" id="properties" class="select">
              <option disabled selected hidden class="select-text">
                Selecione
              </option>
              <% for(let i=0;i<data.length/2;i++) { %>
              <option>
                Cód: <%= data[i].idOportunidade %> - <%= data[i].titulo %>
              </option>
              <% } %>
            </select>
          </form>
        </div>
        <div>
          <button class="button" type="submit">
            <a
              href="/property.ejs"
              style="text-decoration: none; color: inherit"
              >Ver imovél</a
            >
          </button>
        </div>
      </div>
    </div>

    <!-- <script src="index.js"></script>    -->
  </body>
</html>
